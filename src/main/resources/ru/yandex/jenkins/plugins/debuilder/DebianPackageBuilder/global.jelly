<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
  <f:section title="Debian Package Builder">
    <f:entry title="${%Account of GPG key}" field="accountEmail" value="${descriptor.accountEmail}">
      <f:textbox />
    </f:entry>

    <f:entry title="${%Public GPG key}" field="publicKey" value="${descriptor.publicKey}">
      <f:textarea />
    </f:entry>

    <f:entry title="${%Private GPG key}" field="privateKey" value="${descriptor.privateKey}">
      <f:textarea />
    </f:entry>

    <f:entry title="${%GPG passphrase}" field="passphrase" value="${descriptor.passphrase}">
      <f:password />
    </f:entry>
    <f:entry title="Always install sources ?" field="installSources">
      <f:checkbox checked="${instance.installSources}" default="true"/>
    </f:entry>
  </f:section>
</j:jelly>